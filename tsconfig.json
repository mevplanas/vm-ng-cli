{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/options",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "importHelpers": true,
    // "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "target": "es5",
    "lib": ["dom", "es2015.promise", "es5", "es6", "es2017"]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "enableIvy": true
  },
  "files": ["src/app/core/config/config.ts"]
}
